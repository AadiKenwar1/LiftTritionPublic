{
  "cli": {
    "version": ">= 16.10.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "medium"
      },
      "env": {
        "EXPO_NO_WEB": "1"
      },
      "node": "20.11.0"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "medium"
      },
      "env": {
        "EXPO_NO_WEB": "1"
      },
      "node": "20.11.0"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "resourceClass": "medium",
        "credentialsSource": "remote",
        "distribution": "store",
        "image": "latest"
      },
      "env": {
        "EXPO_NO_WEB": "1"
      },
      "node": "20.11.0"
    }
  },
  "submit": {
    "production": {}
  }
}
